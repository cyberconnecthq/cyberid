{
  "name": "cyberid",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:cyberconnecthq/cyberid.git",
  "author": "peng.deng@cyberconnect.me",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint:gh-check": "prettier --check **.{sol,js,md,html}",
    "lint:check": "prettier --check **.{sol,js,md,html} && solhint --config ./config/.solhint.json --ignore-path ./config/.solhintignore -f table 'src/**/*.sol'",
    "lint:fix": "prettier --write **.{sol,js,md}",
    "prepare": "husky install",
    "build": "forge build --force",
    "gen_abi": "ts-node misc/gen_abi.ts",
    "deploy_mocaid:mumbai": "source .env.mumbai && forge script script/DeployMocaId.s.sol:DeployMocaId --verify --rpc-url $RPC_URL --legacy --private-key $PRIVATE_KEY --broadcast -vvv",
    "deploy_deployer:mumbai": "source .env.mumbai && forge script script/DeployCreate2Deployer.s.sol:DeployerCreate2Deployer --verify --rpc-url $RPC_URL --private-key $PRIVATE_KEY --broadcast -vvv",
    "deploy_cyberid:base_goerli": "source .env.base_goerli && forge script script/DeployCyberId.s.sol:DeployCyberId --verify --rpc-url $RPC_URL --private-key $PRIVATE_KEY --broadcast -vvv",
    "deploy_deployer:base_goerli": "source .env.base_goerli && forge script script/DeployCreate2Deployer.s.sol:DeployerCreate2Deployer --verify --rpc-url $RPC_URL --private-key $PRIVATE_KEY --broadcast -vvv",
    "verify_cyberid:base_goerli": "source .env.base_goerli && ETHERSCAN_API_KEY=$ETHERSCAN_API_KEY forge verify-contract --chain-id 84531 --compiler-version v0.8.14+commit.80d49f37 --constructor-args   0x3a772b851560b835a23ae91a21f2014537e254e1 src/core/CyberId.sol:CyberId  --watch",
    "verify_stablefeemw:base_goerli": "source .env.base_goerli && ETHERSCAN_API_KEY=$ETHERSCAN_API_KEY forge verify-contract --chain-id 84531 --compiler-version v0.8.14+commit.80d49f37 --constructor-args 000000000000000000000000cd2a119bd1f7df95d706de6f2057fdd45a0503e20000000000000000000000003a772b851560b835a23ae91a21f2014537e254e1 0x41026000f73f5f59635b489a27ed469a8b58cc66 src/middlewares/StableFeeMiddleware.sol:StableFeeMiddleware  --watch",
    "verify_trustonlymw:base_goerli": "source .env.base_goerli && ETHERSCAN_API_KEY=$ETHERSCAN_API_KEY forge verify-contract --chain-id 84531 --compiler-version v0.8.14+commit.80d49f37 --constructor-args 0000000000000000000000003a772b851560b835a23ae91a21f2014537e254e1 0xeb515b6865d1bfc715a6c22d30258ce385ec43c4 src/middlewares/TrustOnlyMiddleware.sol:TrustOnlyMiddleware  --watch",
    "verify_permissionmw:mumbai": "source .env.mumbai && ETHERSCAN_API_KEY=$ETHERSCAN_API_KEY forge verify-contract --chain-id 80001 --compiler-version v0.8.14+commit.80d49f37 --constructor-args 0000000000000000000000003aa447fcb67a4a1618fe890d41e040246d84d80d 0x1e85945034f9383b1c58c5a567b478c73cc48680 src/middlewares/mocaid/PermissionMw.sol:PermissionMw  --watch",
    "verify_mocaid:mumbai": "source .env.mumbai && ETHERSCAN_API_KEY=$ETHERSCAN_API_KEY forge verify-contract --chain-id 80001 --compiler-version v0.8.14+commit.80d49f37 0xacabdd67166a9b56744d017c270e0fb8769ffba3 src/core/MocaId.sol:MocaId  --watch"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.8.2",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{sol,js,md,html}": "prettier --write"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "tabWidth": 4,
          "printWidth": 80,
          "bracketSpacing": true
        }
      }
    ]
  }
}
