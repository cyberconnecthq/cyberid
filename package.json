{
  "name": "cyberid",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:cyberconnecthq/cyberid.git",
  "author": "peng.deng@cyberconnect.me",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint:gh-check": "prettier --check **.{sol,js,md,html}",
    "lint:check": "prettier --check **.{sol,js,md,html} && solhint --config ./config/.solhint.json --ignore-path ./config/.solhintignore -f table 'src/**/*.sol'",
    "lint:fix": "prettier --write **.{sol,js,md}",
    "prepare": "husky install",
    "build": "forge build --force",
    "gen_abi": "ts-node misc/gen_abi.ts",
    "deploy_mocaid:mumbai": "source .env.mumbai && forge script script/DeployMocaId.s.sol:DeployMocaId --verify --rpc-url $RPC_URL --private-key $PRIVATE_KEY --broadcast -vvv",
    "deploy_deployer:mumbai": "source .env.mumbai && forge script script/DeployCreate2Deployer.s.sol:DeployerCreate2Deployer --verify --rpc-url $RPC_URL --private-key $PRIVATE_KEY --broadcast -vvv",
    "deploy_cyberid:base_goerli": "source .env.base_goerli && forge script script/DeployCyberId.s.sol:DeployCyberId --verify --rpc-url $RPC_URL --private-key $PRIVATE_KEY --broadcast -vvv",
    "deploy_deployer:base_goerli": "source .env.base_goerli && forge script script/DeployCreate2Deployer.s.sol:DeployerCreate2Deployer --verify --rpc-url $RPC_URL --private-key $PRIVATE_KEY --broadcast -vvv",
    "verify_cyberid:base_goerli": "source .env.base_goerli && ETHERSCAN_API_KEY=$ETHERSCAN_API_KEY forge verify-contract --chain-id 84531 --compiler-version v0.8.14+commit.80d49f37 --constructor-args   0x4b22c892796bfcf59406E34f37Db6c8AA73941Ff src/core/CyberId.sol:CyberId  --watch",
    "verify_permissionmw:mumbai": "source .env.mumbai && ETHERSCAN_API_KEY=$ETHERSCAN_API_KEY forge verify-contract --chain-id 80001 --compiler-version v0.8.14+commit.80d49f37 --constructor-args 0000000000000000000000001622b8d366c530635c0072ba969a23c9bde845a5 0xcaeef6a356e83add13025e9dd8a90ee131c06211 src/middlewares/PermissionMw.sol:PermissionMw  --watch",
    "verify_mocaid:mumbai": "source .env.mumbai && ETHERSCAN_API_KEY=$ETHERSCAN_API_KEY forge verify-contract --chain-id 80001 --compiler-version v0.8.14+commit.80d49f37 0xa19577a3ab96a2e71d394ac13f8f3a2b03559ba4 src/core/MocaId.sol:MocaId  --watch"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.8.2",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{sol,js,md,html}": "prettier --write"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "tabWidth": 4,
          "printWidth": 80,
          "bracketSpacing": true
        }
      }
    ]
  }
}
